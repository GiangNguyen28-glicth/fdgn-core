{
  "name": "@fdgn/seed-service",
  "version": "1.0.21",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "build": "rimraf -rf dist && nest build -p tsconfig.json && cpx 'src/**/*.proto' dist",
    "prepublishOnly": "yarn build",
    "dev": "nest start --watch -p tsconfig.json",
    "start": "node dist/main.js",
    "clean:log": "rimraf -rf ./logs",
    "proto:generate": "tsproto  --path ./src/proto --outDir ./src/proto/proto-gen"
  },
  "license": "MIT",
  "dependencies": {
    "@fdgn/common": "^1.1.16",
    "@fdgn/mongoose": "^1.1.16",
    "@fdgn/rabbitmq": "^1.1.16",
    "@fdgn/redis": "^1.1.14",
    "@fdgn/typeorm": "^1.1.16",
    "@grpc/grpc-js": "^1.10.8",
    "@grpc/proto-loader": "^0.7.13",
    "cpx": "^1.5.0",
    "grpc-tools": "^1.12.4",
    "nestjs-proto-gen-ts": "^1.0.21"
  },
  "gitHead": "db2b8443e82168b941be80fafeddb075d370346e"
}
